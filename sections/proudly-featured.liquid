<style>
    /* @import 'nib'; */

.proudly-feature{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.Marquee_announcement_bar {
    /* margin: 30px 0 0 0; */
    background: #ffffff;
    width: 100vw;
    box-sizing: border-box;
    padding: 0.4em 1em;
    color: white;
    font-weight: 400;
    display: flex;
    align-items: center;
    overflow: hidden;
    font-size: calc(var(--typeBaseSize)* .85);
}

.Marquee_content_announcement_bar {
  display: flex;
  animation: Marquee_announcement_bar 20s linear infinite;
}


@media (min-width: 0px) and (max-width: 768px) {
  .Marquee_content_announcement_bar {
    display: flex;
    animation: Marquee_announcement_bar 30s linear infinite;
  }

  
}




.Marquee_content_announcement_bar:hover {
  animation-play-state: paused;
}

.Marquee_tag_announcement_bar {
    display: inline-block;
    white-space: nowrap;
    padding: 1.5rem 1.2em;
    margin: 1.5rem 1.5em;
    text-decoration: none;
    color: white;
    transition: all .2s ease;
}

.Marquee_tag_announcement_bar:hover {
  background: rgba(255, 255, 255, 0.05);
  transform: scale(1.04);
  color: white;
  cursor: pointer;
}

@keyframes Marquee_announcement_bar {
  0% {
    transform: translateX(0);
  }
  100% {
    transform: translateX(-50%);
  }
}

</style>

