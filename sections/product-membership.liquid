<script>
   
        function toggleCard(cardIndex) {
            
            const section = document.getElementById(`section-${cardIndex}`);
            const arrowButton = document.getElementById(`arrow-${cardIndex}`);
            console.log(arrowButton.innerHTML);

            if (section.style.display === "flex") {
                section.style.display = "none";
                arrowButton.innerHTML = `<svg class="fixing_arrow" width="14" height="8" viewBox="0 0 14 8" fill="none" xmlns="http://www.w3.org/2000/svg" transform="rotate(-90)"><path d="M12.7688 0.6705C13.1579 0.957076 13.2411 1.50482 12.9545 1.89393C12.7319 2.19616 12.5092 2.48346 12.314 2.73368C11.9241 3.23319 11.3875 3.90215 10.8044 4.57368C10.2251 5.24086 9.58287 5.93059 8.98324 6.4604C8.68443 6.72442 8.3756 6.96723 8.07445 7.14878C7.79732 7.31584 7.41652 7.5 6.99991 7.5C6.5833 7.5 6.20247 7.31584 5.92534 7.14878C5.62419 6.96723 5.31536 6.72442 5.01655 6.4604C4.41692 5.93059 3.77464 5.24086 3.19535 4.57368C2.6123 3.90215 2.07564 3.23319 1.68581 2.73368C1.49054 2.48346 1.2679 2.19616 1.0453 1.89393C0.758728 1.50483 0.841846 0.957077 1.23095 0.670501C1.38737 0.555303 1.56942 0.499853 1.74984 0.500004L6.9999 0.500004L12.2499 0.500003C12.4304 0.499852 12.6124 0.555302 12.7688 0.6705Z" fill="#14181F"/> </svg>`;
