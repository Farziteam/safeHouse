<style>
       .product-membership{
            padding: 12rem 2rem;
            display: flex;
            flex-direction: column;
            align-items: center;
            background-color: #f9fafb;
            overflow: hidden;
       }

       .member-card-section h2{
        display: -webkit-box;
          -webkit-line-clamp: 2;
          -webkit-box-orient: vertical;  
          overflow: hidden;
          text-overflow: ellipsis;
          max-height: 3em;
          line-height: 1.5em;
       }

        .pricing-container {
            display: flex;
            justify-content: center;
            align-items: center;
            gap: 20px;
            width: 100%;
        }

        .member-card {
            background-color: transparent;
            border-radius: 10px;
            box-shadow: 0 1rem 1.5rem rgba(0, 0, 0, 0.1);
            text-align: center;
            position: relative;
            width: 25%;
            height: 40rem;
            margin: 0.5%;
        }
         .member-card:hover {
            transition: 0.5s;
            transform: scale(1.18);
            border: 0.5rem solid #652fff;
            box-shadow: 0 8px 16px rgba(0, 0, 0, 0.2);
         }

         

         .member-card {
            pointer-events: auto
        }

        .product-membership {
            pointer-events: none
        }
        .product-membership:hover {
           
            padding: 15rem 2rem;
        }

         .member-card:hover .badge{
            display: flex;
         }

         .member-card:hover .pdp-membership-footer{
            display: flex;
         }

         .member-card:hover .pdp-membership-footer{
            display: flex;
         }

         .member-card:hover .member-card-section2 .cta{
            background-color: #652fff;
            color: #ffffff;
         }

         .member-card:hover .member-card-section .memberrship-price{
            font-size: 5rem;
            margin: 0px;
         }

        .member-card:hover .features li span svg path {
            fill: #652fff;
        }


      

        .member-card h2{
            font-size: 2rem;
            font-weight: 600;
            text-align: center;
            margin: 0px;
            margin-top: 1rem;
        }

        .member-card.featured>div>h2{
            
            font-weight: 800;
        }

        .badge {
            position: absolute;
            top: -4.5rem;
            left: 50%;
            transform: translateX(-50%);
            background-color: #652fff;
            color: white;
            padding: 1.5rem 1rem;
            border-radius: 0px;
            border-top-left-radius: 20px;
            border-top-right-radius: 20px;
            font-size: 14px;
            width: 103.2%;
            height: 5rem;
            text-align: left;
            display: none;
            align-items: center;
            gap:0.5rem;
        }

        .discount-badge {
            position: absolute;
            top: -5.5rem;
            right: 1rem;
            color: white;
           
            font-size: 16px;
            border-radius: 5px;
            font-weight: bold;
            
        }

        .memberrship-price {
            font-size: 3rem;
            font-weight: 800;
            color: #333;
            margin: 10px 0;
            text-align: center;
            display: flex;
            align-items: center;
            justify-content: center;
        }

       



        .memberrship-price span {
            color: #636363;
            font-size: 2rem;
            margin-bottom: 1;
        }

        .subtitle {
            font-size: 1.5rem;
            line-height: 2rem;
            color: #B1B1B1;
            margin-bottom: 15px;
            text-align: center;
        }

        .features {
            list-style: none;
            padding: 0;
            margin: 20px 0;
            height: 8rem;
        }

        .features li {
            display: flex;
            align-items: center;
            justify-content: center;
            gap: 0.5rem;
            color: #333;
            font-size: 14px;
            margin: 10px 0;
        }

       

       

        .cta {
            background-color: #f7f4ff;
            color: #3D66FB;
            padding: 10px;
            border-radius: 5px;
            border: 1px solid #3d66fb;
            text-decoration: none;
            text-align: center;
            display: inline-block;
            margin-top: 20px;
            font-size: 18px;
            font-weight: 500;
            width: 100%;
        }

        .pdp-membership-footer{
            position: absolute;
            background-color: #f7f4ff;
            padding: 0.5rem 1rem;
            border: 0.2rem solid #dfdce6;
            border-radius: 10px;
            margin-top: 30px;
            font-size: 14px;
            color: #777;
            width: 285%;
            display: none;
            justify-content: flex-start;
            align-items: center;
            gap: 2rem;
            background-color: #f7f4ff'
        }
       .pdp-membership-footer.mem-foot1{
            top: 99%;
           
        
       }

       

        .pdp-membership-footer.mem-foot2{
            top: 99%;
            left: -90%;
        }

        .pdp-membership-footer.mem-foot3{
            top: 99%;
            left: -185%;
        }

        .pdp-membership-footer p{
            margin: 0px;
        }

        .member-card-section{
            display: grid;
            grid-template-columns: 1fr;
            padding-top: 2rem;
            min-height: 40%;
            border-radius: 1rem;
            background-color: #ffffff;
        }

        .member-card-section2{
            display: flex;
            flex-direction: column;
            height: 60%;
            padding: 2rem 3rem;
        }
        .stickers{
            background-color: #f7f4ff;
            display: flex;
            justify-content: flex-end;
            padding: 0.5rem;
            border-bottom-left-radius: 1rem;
            border-bottom-right-radius: 1rem;
            height: 3rem;
        }

        .member-card .stickers{
            display: none;
        }
        .member-card.featured .stickers{
            display: flex;
            gap: 1rem;
        }
        .expand-button{
            background-color: #d9d9d9;
            padding: 1rem;
            border-radius: 50%;
            width: 3rem;
            height: 3rem;
            display: none;
           
        }

        @media screen and (max-width: 768px){
            .product-membership{
                padding: 4rem 2rem;    
            }
            .pricing-container{
                flex-direction: column;
            }

            .member-card{
                width: 98%;
                height: 80%;
                border: 1px solid #e5e5e5;
            }
            
            .member-card:hover{
                transform: scale(1);
                margin: 4rem 0px;
                
            }
            .pdp-membership-footer{
                margin: 1px;
                width: 90%;
                top: 102%;
                padding: 0.5rem;
                left: 5%;
                text-align: left;
                font-size: 10px;
            }

            .pdp-membership-footer.mem-foot1{
                top: 99%;                
            }

            .pdp-membership-footer.mem-foot2{
                top: 99%;
                left: 5%;
            }

            .pdp-membership-footer.mem-foot3{
                top: 99%;
                left: 5%;
            }

            .member-card-section{
                min-height: 14rem;
            }
            .member-card-section h2{
                display: none;
            }
            .member-card-section2{
                display: none;
            }

            .stickers{
                justify-content: flex-start;
            }

            .member-card .stickers{
                display: flex;
            }

            .badge{
                width: 103%;
            }

            .expand-button{
                display: flex;
                justify-content: center;
                align-items: center;
                position: absolute;
                top: 12rem;
                left: 85%;
            }

            .subtitle{
                margin: 0px;
            }
           
            .fixing_arrow{
                pointer-events: none;
            }
        } 

    </style>
