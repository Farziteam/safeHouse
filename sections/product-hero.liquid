<style>
  .hero-section {
    position: relative;
    width: 100%;
    height: 100vh;
    background-size: cover;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    color: white;
    text-align: left;
  }

  .hero-section::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, {{ section.settings.overlay_opacity }}); /* Dynamic overlay */
  }

  .hero-content {
    padding: 8rem;
    position: relative;
    z-index: 2;
  }

  .hero-content h1 {
    font-size: 48px;
    font-weight: 700;
    margin-bottom: 20px;
    color: white;
  }

  .hero-content p {
    font-size: 18px;
    font-weight: 400;
    margin-bottom: 30px;
    opacity: 70%;
  }

  .cta-button {
    padding: 12px 30px;
    font-size: 16px;
    font-weight: 500;
    border-radius: 5px;
    text-decoration: none;
    border: none;
    cursor: pointer;
    transition: background-color 0.3s ease-in-out;
    #e4dbd2
  }

  .cta-button:hover {
    background-color: darken({{ section.settings.cta_button_bg_color }}, 10%);
  }

  .banner-head-text{
      font-family: Tomato Grotesk;
      font-weight: 500;
      font-size: 3.8rem;
      line-height: 4.2rem;
      text-align: center;
      letter-spacing: -1.74px;
      margin: 2rem 0rem;
    }
    .banner-subhead{
      font-size: 1.5rem;
      font-weight: 400;
      line-height: 3rem;
    }
    .get-button{
      display: flex;
      align-items: center;
      justify-content: space-between;
      gap: 1rem;
      padding: 10px 20px;
      border-radius: 10px;
      background-color: #652fff;
      color: #FFFFFF;
      text-decoration: none;
      font-family: Inter;
      font-weight: 400;
      font-size: 18px;
    }

    .btn-section{
      display: flex;
      justify-content: flex-start;
      align-items: center;
      margin: 2rem 0rem;
    }

    .social-icon{
      position: relative;
      width: 15rem;
      margin-right: -3rem;
      margin-left: 2rem;
    }
    .social-media{
        display: flex;
        justify-content: space-around;
        margin-top: 4rem;
        width: 80%;
    }

    


  /* Responsive Styles */
  @media screen and (max-width: 768px) {
    .hero-section{
        background-position: center;
        height: 80%;
        justify-content: center;
    }
    .hero-content {
        display: flex;
        flex-direction: column;
        padding: 2rem;
    }

    .get-button{
      width: 50%;
    }
    .social-media{
        margin-top: 0px;
        width: 100%;
    }
    .hero-content h1 {
      font-size: 36px;
    }

    .hero-content p {
      font-size: 16px;
    }
    .btn-section{
        flex-direction: column;
        gap: 2rem;
    }

    .cta-button {
      padding: 10px 20px;
    }
  }

  @media screen and (max-width: 480px) {
    .hero-content h1 {
      font-size: 28px;
    }

    .hero-content p {
      font-size: 14px;
    }

    .cta-button {
      padding: 8px 18px;
    }
  }
</style>

